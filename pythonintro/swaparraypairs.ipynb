    "def swap(list):\n",
    "    for i in range(0,len(list)-1,2):\n",
    "        list[i+1],list[i] = list[i],list[i+1]\n",
    "    print()\n",
    "    return list\n",
    "\n",
    "size = int(input())\n",
    "list = [int(i) for i in input().split()]\n",
    "swap(list)\n",
    "print(list)"
