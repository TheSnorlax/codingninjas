    "def removeConsequtive(s):\n",
    "    i = 1\n",
    "    while(i<len(s)):\n",
    "        if s[i] == s[i-1]:\n",
    "            s = s[:i] + s[(i+1):]\n",
    "        else:\n",
    "            i+=1\n",
    "    return s\n",
    "\n",
    "s = input()\n",
    "s = removeConsequtive(s)\n",
    "print(s)\n",
    "        "
   ]
